<animation>
    <type>rotate</type>
    <object-name>pedalR</object-name>
    <property>controls/flight/rudder</property>
    <factor> 20 </factor>
    <center>
	<x-m>-0.8062</x-m>
	<y-m> 0</y-m>
	<z-m>-0.3178</z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> -1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pedalL</object-name>
    <property>controls/flight/rudder</property>
    <factor> 20 </factor>
    <center>
	<x-m>-0.8062</x-m>
	<y-m> 0</y-m>
	<z-m>-0.3178</z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> 1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  

  <animation>
    <type>select</type>
    <object-name>rotor1</object-name>
    <object-name>rotor2</object-name>
    <condition>
      <less-than>
        <property>rotors/main/rpm</property>
        <value>350.001</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>rotor1blur</object-name>
    <object-name>rotor2blur</object-name>
    <condition>
      <greater-than>
        <property>rotors/main/rpm</property>
        <value>150</value>
      </greater-than>
    </condition>
  </animation>


  <animation>
    <type>select</type>
	<object-name>tail1</object-name>
	<object-name>tail2</object-name>
	<object-name>tail3</object-name>
    <condition>
      <less-than>
        <property>rotors/tail/rpm</property>
        <value>150.001</value>
      </less-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>tailblur</object-name>
    <condition>
      <greater-than>
        <property>rotors/tail/rpm</property>
        <value>150</value>
      </greater-than>
    </condition>
  </animation>
  
<animation>
    <type>select</type>
	<object-name>manche1</object-name>	
	<object-name>manche2</object-name>
    <condition>
      <greater-than>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>
  

